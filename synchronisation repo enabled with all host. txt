for host in $(hammer host list --organization "ACME" --search "lifecycle_environment = DEV" --fields Name --no-headers); do
  hammer host subscription set-repository-override \
    --organization "ACME" \
    --name "$host" \
    --content-label "rhel-9-for-s390x-baseos-rpms" \
    --value 1
done
----
ORG="obsibmz"; LCE="dev"; LABEL="rhel-9-for-s390x-baseos-eus-rpms"
for H in $(hammer host list --organization "$ORG" --search "lifecycle_environment = $LCE" --fields Name --no-headers); do
  hammer host subscription enabled-repositories --host "$H" --enable "$LABEL"
done